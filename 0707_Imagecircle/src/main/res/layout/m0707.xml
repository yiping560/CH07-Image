<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Silver"
    android:padding="@dimen/p10"
    tools:context=".M0707">

    <TextView
        android:id="@+id/m0707_t001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="56dp"
        android:layout_marginBottom="205dp"
        android:text="@string/m0707_t001"
        android:textSize="@dimen/fontsize" />

    <TextView
        android:id="@+id/myname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="29dp"
        android:text="@string/myname"
        android:textColor="@color/Yellow"
        android:textSize="@dimen/fontsize" />

    <tw.tcnr07.m0707.CircleImgView
        android:id="@+id/m0707_imgview"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="173dp"
        android:layout_marginEnd="125dp"
        android:background="#00000000"
        android:scaleType="matrix"
        app:srcCompat="@drawable/img15th" />

    <tw.tcnr07.m0707.CircleImgbutton
        android:id="@+id/m0707_imgbutton"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="134dp"
        android:layout_marginEnd="271dp"
        android:background="#00000000"
        android:scaleType="matrix"
        app:srcCompat="@drawable/img16th"
        tools:ignore="SpeakableTextPresentCheck" />

</RelativeLayout>